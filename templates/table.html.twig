<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        Таблица {{ name }} {% if page is defined %} Страница {{page}} {% endif %}
        {% if table is defined %}
            <tbody>
                {% for elem in table %}
                <tr class="table_column"> 
                    <div>
                        <a href="/{{name}}/{{elem.id}}">{{elem}}</a>
                    </div>
                </tr>
                {% endfor %}
            </tbody>
           
        {% elseif paginated %}
            {% for elem in paginated %}
                {{elem}}
            {% endfor%}
        {% endif %}
    </body>
</html>